{
	"manifest_version": 2,
	"name": "PDF drive Search",
	"short_name":"PDFdrive-search",
	"description": "__MSG_description__",
	"version": "1.0",
	"default_locale": "en",
	"icons": {
		"128": "img/128.png",
		"96": "img/96.png",
		"64": "img/64.png",
		"48": "img/48.png",
		"32": "img/32.png",
		"16": "img/16.png"
	},
	"web_accessible_resources": ["img/*.png"],
	"permissions": ["contextMenus", "tabs"],
	"background": {
		"scripts": ["main.js"]
	}
}
 